
  <span style="color:white">Users page.</span>
  <br>
  <caption style="font-size:25px; font-color:white; text-align: center">Users</caption>
  <table style="width:70%">
    <th>Name</th>
    <th>Email</th>
    <th>Username</th>
    <th>About</th>
    <% for (var i = 0; i < users.length; i++) {%>
      <tr>
        <td>
          <%= users[i].name %>
            <td/>
            <td>
              <%= users[i].email %>
            </td>
            <td>
              <%= users[i].password %>
            </td>
            <td>
              <%= users[i].about %>
            </td>
            <td>
              <%= users[i].role %>
            </td>
            <td>
              <%= users[i].id %>
            </td>
      </tr>
      <% } %>
  </table>
  <br>
  <div class="list-group">
  <a href="#" class="list-group-item active">
    Users
  </a>
  <a href="#" class="list-group-item"><%= users[i].name %></a>
  <a href="#" class="list-group-item"><%= users[i].email %></a>
  <a href="#" class="list-group-item"><%= users[i].password %></a>
  <a href="#" class="list-group-item"><%= users[i].about %></a>
</div>
