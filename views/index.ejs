    <!-- Welcome Section Starts -->
    <section id="about" class="section-area welcome-section">
        <!-- Nested Container Starts -->
        <div class="container text-center">
            <!-- Heading Starts -->
            <h2 class="section-main-heading text-center"><span>Welcome To Your<strong class="text-normal text-spl-color"> Easy Meal Planning</strong> App</span></h2>
            <!-- Heading Ends -->
            <!-- Chef Icon Starts -->
            <p><img src="images/icons/chef-icon.png" alt="Chef" class="img-center"></p><br>
            <!-- Chef Icon Ends -->
            <!-- Nested Row Starts -->
            <div class="row">
                <!-- Col#1 Starts -->
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="welcome-col">
                        <h3>Kid Friendly</h3>
                        <p><img src="images/icons/section-icon1.png" alt="Fruit" class="img-center"></p>
                        <p>
                            Kids get to make their voices heard.
                        </p>
                    </div>
                </div>
                <!-- Col#1 Ends -->
                <!-- Col#2 Starts -->
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="welcome-col">
                        <h3>Plan Your Meals</h3>
                        <p><img src="images/icons/section-icon2.png" alt="Organic Farms" class="img-center"></p>
                        <p>
                            Get your grocery list ahead of time.
                        </p>
                    </div>
                </div>
                <!-- Col#2 Ends -->
                <!-- Col#3 Starts -->
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="welcome-col">
                        <h3>Streamline</h3>
                        <p><img src="images/icons/section-icon3.png" alt="Vegetables" class="img-center"></p>
                        <p>
                            A quick and easy format for deciding on dinner.
                        </p>
                    </div>
                </div>
                <!-- Col#3 Ends -->
                <!-- Col#4 Starts -->
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="welcome-col">
                        <h3>Dinner Democracy</h3>
                        <p><img src="images/icons/section-icon4.png" alt="Nutrition Facts" class="img-center"></p>
                        <p>
                            Everyone gets a vote.
                        </p>
                    </div>
                </div>
                <!-- Col#4 Ends -->
            </div>
            <!-- Nested Row Ends -->
        </div>
        <!-- Nested Container Ends -->
    </section>
    <!-- Welcome Section Ends -->



    <!-- Order Section Starts -->
    <section id="login" class="section-area inverse order-section parallax">
        <!-- Nested Container Starts -->
        <div class="container">
            <!-- Nested Row Starts -->
            <div class="row">
                <!-- Features Start -->
                <div class="col-md-8 col-sm-7 col-xs-12" id="features-list">
                    <h1>Join us...and make dinner manageable again.</h1>
                    <p class="lead">Your dinner time should be spent connecting with your family, not debating the daily menu. Create a profile and start your journey to a stress-free evening.</p>
                    <ul class="list-unstyled row features-list">
                        <li class="col-sm-6 col-xs-12 clearfix">
                            <i class="fa fa-clock-o pull-left"></i>
                            <h4>Save Time</h4>
                            <p>When you know what's for dinner, you can plan accordingly.</p>
                        </li>
                        <li class="col-sm-6 col-xs-12 clearfix">
                            <i class="fa fa-calendar pull-left"></i>
                            <h4>Daily Plans</h4>
                            <p>With daily meal planning options, there isn't any confusion.</p>
                        </li>
                    </ul>
                </div>
                <!-- Features Ends -->
                <!-- Login and signup boxes begin -->
                <div class="row">
                    <div class="col-sm-5">

                        <div class="form-box">
                            <div class="form-top">
                                <div class="form-top-left">
                                    <h3>Please login to our site</h3>
                                    <p>Enter username and password to log on:</p>
                                </div>
                                <div class="form-top-right">
                                    <i class="fa fa-lock"></i>
                                </div>
                            </div>
                            <div class="form-bottom">
                                <form role="form" action="/" method="post" class="login-form">
                                    <div class="form-group">
                                        <label class="sr-only" for="form-username">Email</label>
                                        <input type="text" name="email" placeholder="Email" class="form-username form-control" id="form-username">
                                    </div>
                                    <div class="form-group">
                                        <label class="sr-only" for="form-password">Password</label>
                                        <input type="password" name="password" placeholder="Password" class="form-password form-control" id="form-password">
                                    </div>
                                    <button type="submit" class="btn">Sign in!</button>
                                </form>
                            </div>
                        </div>
                        <!-- Social-login begins here -->
                        <div class="social-login">
                            <h3>...or login with:</h3>
                            <div class="social-login-buttons">
                                <a class="btn btn-link-2" href="/auth/facebook">
                                    <i class="fa fa-facebook">  </i>Facebook
                                    <a class="btn btn-link-2" href="#">
                                        <i class="fa fa-google">  </i>Google
                                    </a>
                            </div>
                        </div>

                    </div>

                    <div class="col-sm-1 middle-border"></div>
                    <div class="col-sm-1"></div>

                    <div class="col-sm-5">

                        <div class="form-box">
                            <div class="form-top">
                                <div class="form-top-left">
                                    <h3>Sign up now</h3>
                                    <p>Fill in the form below to get instant access:</p>
                                </div>
                                <div class="form-top-right">
                                    <i class="fa fa-pencil"></i>
                                </div>
                            </div>
                            <div class="form-bottom">
                                <form role="form" action="/signup" method="post" class="registration-form">
                                    <div class="form-group">
                                        <label class="sr-only" for="form-first-name">Name</label>
                                        <input type="text" name="name" placeholder="Name" class="form-first-name form-control" id="form-first-name">
                                    </div>
                                    <div class="form-group">
                                        <label class="sr-only" for="form-last-name">Email</label>
                                        <input type="text" name="email" placeholder="Email" class="form-last-name form-control" id="form-last-name">
                                    </div>
                                    <div class="form-group">
                                        <label class="sr-only" for="form-email">Password</label>
                                        <input type="text" name="password" placeholder="Password" class="form-email form-control" id="form-email">
                                    </div>
                                    <div class="form-group">
                                        <label class="sr-only" for="form-about-yourself">About yourself</label>
                                        <textarea name="about" placeholder="Your favorite food?" class="form-about-yourself form-control" id="form-about-yourself"></textarea>
                                    </div>
                                    <!-- Terms & Conditions Starts -->
                                    <div class="checkbox">
                                        <label>
                    <input type="checkbox"> I Agree to the Terms &amp; Conditions.
                  </label>
                                    </div>
                                    <!-- Terms & Conditions Ends -->
                                    <button type="submit" class="btn">Sign me up!</button>
                                </form>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- Login and signup ends -->
